<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="GdwmpCoreSkin">

 <object name="gdwmp_core_images"
    meta_type="Filesystem Directory View"
    directory="gdwmp.core:skins/gdwmp_core_images"/>
 <object name="gdwmp_core_scripts"
    meta_type="Filesystem Directory View"
    directory="gdwmp.core:skins/gdwmp_core_scripts"/>

 <skin-path name="GdwmpCoreSkin" based-on="Sunburst Theme">
  <layer name="gdwmp_core_images"
     insert-after="custom"/>
  <layer name="gdwmp_core_scripts"
     insert-after="gdwmp_core_images"/>
 </skin-path>

</object>
